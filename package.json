{
  "name": "typescript-interview-challenges",
  "version": "1.0.0",
  "description": "TypeScript interview challenges for junior developers",
  "private": true,
  "workspaces": [
    "challenges/*"
  ],
  "scripts": {
    "install:all": "npm install && npm run install:challenges",
    "install:challenges": "npm install --workspaces",
    "build:all": "npm run build --workspaces",
    "test:all": "npm run test --workspaces",
    "lint:all": "npm run lint --workspaces",
    "format:all": "npm run format --workspaces"
  },
  "keywords": ["typescript", "interview", "challenges", "junior-developer"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0",
    "prettier": "^3.0.0"
  }
}